{
  "name": "@qordli/next-express",
  "private": false,
  "version": "0.1.6",
  "type": "module",
  "description": "A file-based routing compiler for express, inspired by Next.js.",
  "author": "ZEHUA YANG <march772827981@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qordli/next-express.git"
  },
  "bin": {
    "next-express": "dist/bin/next-express.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./build": {
      "types": "./dist/build.d.ts",
      "import": "./dist/build.js",
      "require": "./dist/cjs/build.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "postinstall": "node dist/post-install/index.js",
    "dev": "tsx src/index.ts",
    "snapshots:update": "tsx tests/scripts/update-custom-snapshot.ts",
    "test": "vitest",
    "build": "tsx scripts/build.ts",
    "build:skip-rs-impl": "SKIP_BUILD_RUST_IMPL=1 tsx scripts/build.ts",
    "release": "tsx scripts/release.ts ",
    "tsup": "tsup"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.3",
    "chokidar": "^5.0.0",
    "clipanion": "4.0.0-rc.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.2.1",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "smol-toml": "^1.6.0",
    "socket.io": "^4.8.3",
    "ts-morph": "^27.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "chokidar": ">=4.0.3",
    "express": ">=5.1.0",
    "ts-morph": ">=26.0.0",
    "tsup": ">=8.5.0"
  }
}
