{
  "name": "@qordli/next-express",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "description": "A file-based routing compiler for express, inspired by Next.js.",
  "author": "ZEHUA YANG <march772827981@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qordli/next-express.git"
  },
  "bin": {
    "next-express": "dist/bin/next-express.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./build": {
      "import": "./dist/build.js",
      "require": "./dist/cjs/build.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "postinstall": "node dist/post-install/index.js",
    "dev": "tsx src/index.ts",
    "snapshots:update": "tsx tests/scripts/update-custom-snapshot.ts",
    "test": "vitest",
    "build": "tsx scripts/build.ts",
    "release": "tsx scripts/release.ts ",
    "tsup": "tsup"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "chokidar": "^4.0.3",
    "clipanion": "4.0.0-rc.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "smol-toml": "^1.4.2",
    "socket.io": "^4.8.1",
    "ts-morph": "^26.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "chokidar": "^4.0.3",
    "express": "^5.1.0",
    "ts-morph": "^26.0.0",
    "tsup": "^8.5.0"
  }
}
